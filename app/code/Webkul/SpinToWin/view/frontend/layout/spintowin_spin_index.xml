        <referenceContainer name="main.content" htmlClass="page-main" />
        <referenceBlock name="page.main.title" remove="true" />
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="spintowin_index" template="Webkul_SpinToWin::spin_index.phtml"/>
        </referenceContainer>
    </body>
