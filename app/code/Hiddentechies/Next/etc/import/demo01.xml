<root>
    <config>
        <web>
            <default>
                <cms_home_page>next_theme_home</cms_home_page>
            </default>
        </web>
        <cms>
            <wysiwyg>
                <enabled>hidden</enabled>
            </wysiwyg>
        </cms>
        <nextsettings>
            <htinformation>
                <activationproductsku>M2HTBK</activationproductsku>
            </htinformation>
            <navigation_settings>
                <enable>1</enable>
            </navigation_settings>
            <promotion_settings>
                <enable_promotion>1</enable_promotion>
                <promotion_text>Next - The #1 Multipurpose Magento 2 Theme.</promotion_text>
            </promotion_settings>
            <category_settings>
                <show_border>0</show_border>
                <show_new_label>0</show_new_label>
                <show_sale_label>0</show_sale_label>
            </category_settings>
            <product_settings>
                <show_new_label>0</show_new_label>
                <show_sale_label>0</show_sale_label>
            </product_settings>
            <newsletter_settings>
                <enable>1</enable>
                <title>Mauris mollis eros neque, vel auctor magna pulvina blandit dign</title>
            </newsletter_settings>
            <footer_columns>
                <enable_footer_columns>1</enable_footer_columns>
                <footer_st_column_select>staticblock</footer_st_column_select>
                <footer_st_column>footer_quicklinks</footer_st_column>
                <footer_nd_column>footer_myaccount</footer_nd_column>
                <footer_rd_column></footer_rd_column>
            </footer_columns>
            <footer_contant>
                <address>265, Lorem Street, New York, 32456, United States</address>
                <email>info@yourdomain.com</email>
                <phone>+1 234 567 8901</phone>
            </footer_contant>
            <footer_bottom>
                <enable_footer_bottom>1</enable_footer_bottom>
                <copyright_text>Copyright 2022 Next. All rights reserved.</copyright_text>
                <copyright_right_block>payment-icons</copyright_right_block>
            </footer_bottom>
            <customcss>
                <custom_css></custom_css>
            </customcss>
        </nextsettings>
    </config>
</root>