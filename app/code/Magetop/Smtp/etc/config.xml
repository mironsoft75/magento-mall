<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <smtp>
            <module>
                <type>1</type>
            </module>
            <general>
                <enabled>0</enabled>
                <log_email>1</log_email>
                <clean_email>10</clean_email>
            </general>
        </smtp>
        <email_marketing>
            <general>
                <enabled>0</enabled>
                <is_tracking>0</is_tracking>
                <newsletter_subscriber>0</newsletter_subscriber>
                <synchronization>
                    <sync_type>1</sync_type>
                    <sync_options>all</sync_options>
                    <days_range>lifetime</days_range>
                </synchronization>
            </general>
        </email_marketing>
    </default>
</config>
